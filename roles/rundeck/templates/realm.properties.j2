# Rundeck User Authentication File
# Managed by Ansible - DO NOT EDIT MANUALLY
#
# Format: username: password,role1,role2,...
# Available roles: admin, user, architect, deploy, build
#
# Password format options:
# - Plain text (default): username: password,roles
# - MD5: username: MD5:hash,roles
# - BCrypt: username: bcrypt:hash,roles
#
{% for user in rundeck_users %}
{{ user.username }}: {{ user.password }},{{ user.roles | default('user') }}
{% endfor %}
